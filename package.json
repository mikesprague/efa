{
  "name": "extrafood",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.x"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "scripts": {
    "assets": "webpack --display-max-modules 0 --color true",
    "assets:watch": "yarn assets -w",
    "build": "yarn assets -p",
    "build:dev": "yarn assets -d",
    "deploy:dev": "yarn build && now",
    "dev": "concurrently 'yarn watch' 'yarn assets:watch -d --color true --display minimal' --names 'ðŸ–¥ ,ðŸ“¦ ' --prefix '[{name}] '",
    "start": "node ./index.js",
    "watch": "nodemon ./index.js --ignore public/",
    "watch:debug": "nodemon ./index.js --inspect --ignore public/",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect"
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      "defaults"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "@bugsnag/node": "^6.5.0",
    "@bugsnag/plugin-express": "^6.5.0",
    "@google/maps": "^1.0.2",
    "axios": "^0.19.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.18",
    "dompurify": "^2.0.7",
    "dotenv": "^8.2.0",
    "es6-promisify": "^6.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.0",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "html-to-text": "^5.1.1",
    "jimp": "^0.9.3",
    "juice": "^6.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.8.3",
    "mongoose-mongodb-errors": "^0.0.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "pug": "^2.0.4",
    "register-service-worker": "^1.6.2",
    "slugs": "^0.1.3",
    "snyk": "^1.266.0",
    "uuid": "^3.3.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "compression-webpack-plugin": "^3.0.1",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "now": "^16.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpackbar": "^4.0.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "import",
      "html",
      "markdown",
      "prettier"
    ],
    "rules": {
      "func-names": "off",
      "no-console": "warn",
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "res|next|^err"
        }
      ],
      "max-len": "off",
      "no-underscore-dangle": "off",
      "space-before-function-paren": "off"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "plugins": [
      "all"
    ],
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "snyk": true
}
