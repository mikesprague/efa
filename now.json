{
  "version": 2,
  "name": "extrafood",
  "builds": [
    {
      "src": "./index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "public/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/login",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/loggedIn",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/logout",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/register",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/account",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/account/forgot",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/account/reset/*",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/notifications",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/user",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ],
  "env": {
    "MONGO_DB_URL": "@mongo-db-url",
    "MAIL_USER": "@mail-user",
    "MAIL_PASS": "@mail-pass",
    "MAIL_HOST": "@mail-host",
    "MAIL_PORT": "@mail-port",
    "GOOGLE_MAPS_API_KEY": "@google-maps-api-key",
    "PORT": "3000",
    "APP_SESSION_SECRET": "@app-session-secret",
    "APP_SESSION_KEY": "@app-session-key",
    "TWILIO_AUTH_TOKEN": "@twilio-auth-token",
    "TWILIO_ACCOUNT_SID": "@twilio-account-sid",
    "TWILIO_NUMBER": "@twilio-number",
    "BUGSNAG_KEY": "@bugsnag-key"
  }
}
